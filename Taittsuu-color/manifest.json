 {
   "name": "Taittsuu-color",
   "author": "taka_taka",
   "description": "chrome extension for customize CSS and JS.",
   "version": "1.0.0",
   "manifest_version": 2,
   "web_accessible_resources": ["*"],
   "permissions": ["storage"],
   "content_scripts": [
    {
      "matches": ["https://taittsuu.com/home/*","https://taittsuu.com/users/*",https://taittsuu.com/publictimeline/*"]
      "css": ["Taittsuu/style.css"],"js": ["Taittsuu/script.js","Taittsuu/style.css"]
    
    }
  ]
}